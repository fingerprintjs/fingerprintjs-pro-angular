{
  "name": "fingerprintjs-pro-angular-demo",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build fingerprintjs-pro-angular && ./bin/copy_assets.sh",
    "watch": "ng build --watch --configuration development",
    "watch:library": "ng build fingerprintjs-pro-angular --watch",
    "dev:ssr": "ng run fingerprintjs-pro-angular-demo:serve-ssr",
    "serve:ssr": "node dist/fingerprintjs-pro-angular-demo/server/main.js",
    "build:ssr": "ng build && ng run fingerprintjs-pro-angular-demo:server",
    "prerender": "ng run fingerprintjs-pro-angular-demo:prerender",
    "generate:version": "node bin/generate-version.js",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "eslint --ext .js,.ts --ignore-path .gitignore --max-warnings 0 --fix .",
    "test:dts": "tsc --noEmit --isolatedModules dist/fingerprintjs-pro-angular/fingerprintjs-fingerprintjs-pro-angular.d.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "docs": "typedoc projects/fingerprintjs-pro-angular/src/public-api.ts --out docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nguniversal/express-engine": "13.1.1",
    "express": "^4.15.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.6",
    "@angular-eslint/schematics": "^13.2.1",
    "@angular/cli": "~13.3.6",
    "@angular/compiler-cli": "~13.3.0",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "commitizen": "^4.2.5",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "jasmine-core": "~4.0.0",
    "jest": "^28.1.0",
    "jest-preset-angular": "^12.0.1",
    "lint-staged": "^13.0.3",
    "ng-packagr": "^13.0.0",
    "prettier": "^2.6.2",
    "typedoc": "^0.24.8",
    "typescript": "~4.6.2"
  },
  "lint-staged": {
    "*.ts": "yarn run lint:fix",
    "*.tsx": "yarn run lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "workspaces": [
    "projects/*"
  ]
}
